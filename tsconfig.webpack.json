{
    "compilerOptions": {
        "resolveJsonModule": true,
        "target": "ES2017",
        "module": "EsNext",
        "moduleResolution": "Node",
        "rootDir": "./src",
        "outDir": "./dist",
        "lib": ["ES2019", "DOM", "DOM.Iterable", "ES2020"],
        "downlevelIteration": true,
        "esModuleInterop": true,
        "jsx": "react",
        "jsxFactory": "React.createElement",
        "jsxFragmentFactory": "React.Fragment",
        "noEmit": true
    },
    "exclude": [
        "dist/",
        "webpack.config.ts",
        "test/"
    ]
}